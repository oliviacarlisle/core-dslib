{
  "name": "array-queue",
  "version": "1.0.0",
  "description": "A simple, efficient queue implementation using a circular buffer",
  "type": "module",
  "scripts": {
    "test": "jest",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" \"bench/**/*.ts\"",
    "build": "rm -rf dist && tsc -p tsconfig.dist.json",
    "bench": "tsx bench/Queue.bench.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oliviacarlisle/ts-data-structures.git"
  },
  "keywords": [
    "queue",
    "fifo",
    "enqueue",
    "dequeue"
  ],
  "author": "Olivia Carlisle",
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./core/*": {
      "types": "./dist/core/*.d.ts",
      "import": "./dist/core/*.js"
    }
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "bugs": {
    "url": "https://github.com/oliviacarlisle/ts-data-structures/issues"
  },
  "homepage": "https://github.com/oliviacarlisle/ts-data-structures#readme",
  "devDependencies": {
    "@types/jest": "^29.5.13",
    "@types/node": "^22.7.4",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "ts-jest": "^29.2.5",
    "typescript": "^5.6.3"
  }
}
